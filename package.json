{
    "name": "pagy",
    "version": "0.0.1",
    "description": "SvelteJS + Spectre client, Derver BFF server, Redis base & JWT auth simple headless CMS, bundeled with ESBuild",
    "main": "server/server.js",
    "scripts": {
        "build": "node esbuild",
        "dev": "NODE_ENV=dev node esbuild --dev",
        "prestart": "node esbuild",
